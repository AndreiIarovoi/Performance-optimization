{
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "types": [
      "core-js"
    ]
  }
}
